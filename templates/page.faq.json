          "type": "text",
          "settings": {
            "text": "<p>Please check the FAQ below for answers to commonly asked questions. Need more help? Email us at <a href=\"mailto:sale@cosmet.com\" title=\"mailto:sale@cosmet.com\">sale@cosmet.com<\/a><\/p>"
          }
